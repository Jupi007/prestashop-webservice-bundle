{
    "name": "jupi007/prestashop-webservice-bundle",
    "description": "Symfony integration for PrestaShop Webservice lib",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jupi007",
            "email": "paul.kepinski@orange.fr"
        }
    ],
    "require": {
        "prestashop/prestashop-webservice-lib": "dev-master",
        "jupi007/prestashop-webservice-lib-extra": "0.*"
    },
    "require-dev": {
        "symfony/framework-bundle": "5.2.*"
    },
    "autoload": {
        "psr-4": {
            "Jupi007\\PrestashopWebserviceBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jupi007\\PrestashopWebserviceBundle\\Tests\\": "tests/"
        }
    }
}
